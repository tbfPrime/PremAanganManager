<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="25.0">
         <children>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <ComboBox fx:id="searchCategoryComboBox" prefHeight="31.0" prefWidth="168.0" promptText="Search Category" />
                  <TextField fx:id="searchField" HBox.hgrow="ALWAYS" />
                  <Button fx:id="searchButton" mnemonicParsing="false" text="Search" onAction="#searchAction" />
                  <HBox fx:id="searchFilterButtonHBox" alignment="CENTER_RIGHT" >
                     <children>
                        <Button fx:id="searchFilterResetButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Reset Filters" HBox.hgrow="ALWAYS" onAction="#searchFilterResetAction" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </HBox>
            <VBox fx:id="searchFilterVBox" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
